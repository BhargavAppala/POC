<script src="@ViewBag.AnalyticsBaseUrl"></script>

<script>
    (function () {
        SCBeacon = window.SCBeacon || [];
        SCBeacon.push(['ready', function () {
            @if (ViewBag.DebugEnabled == true)
            {
                @:window.console.log("Sitecore Beacon API - Load Successful");
            }
        }]);
        SCBeacon.push(['error', function () {
            @if (ViewBag.DebugEnabled == true)
            {
                @:window.console.log("Sitecore Beacon API - Load Unsuccessful");
            }
        }]);
    })();
</script>
