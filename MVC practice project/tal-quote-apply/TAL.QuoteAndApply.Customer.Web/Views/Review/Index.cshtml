@using TAL.QuoteAndApply.Analytics.Models

@{
    ViewBag.Title = "Review Cover";
    ViewBag.AppName = "Review";
    ViewBag.ShowSpinnerOnStartup = "true";
    ViewBag.StepIndex = 3;
    ViewBag.PageCssClass = "page-review";
}

@section Analytics
{
    @{
        Html.RenderAction("Header", "Analytics", new { model = new AnalyticsPageModel("ReviewCoverPage", ViewBag.Title, "Quote", DateTime.Now, DateTime.Now) });
    }
}

<section class="tal-section-review">
    @{
        Html.RenderPartial("~/Views/Shared/_PageHeader.cshtml");
    }
    <div class="container">
        <main class="tal-layout-main">
            <tal-review-form></tal-review-form>
        </main>
    </div>
</section>


